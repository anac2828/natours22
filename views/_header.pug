header.header
  nav.nav.nav--tours
    a.nav__el(href='/') All tours
  .header__logo
    img(src='/img/logo-white.png' alt='Natours logo')
  nav.nav.nav--user
    if user
      - console.log(user)
      a.nav__el(href='#') My bookings
      a.nav__el(href='#')
        img.nav__user-img(src=`img/users/${user.photo}` alt='User photo')
        span= user.name.split(' ')[0]
      a.nav__el.nav__el--cta(href="/login") Log out
    else
      a.nav__el.nav__el--cta(href="/login") Log in
      a.nav__el.nav__el--cta(href="/signup") Sign up